<?xml version="1.0" encoding="UTF-8"?>
<SQL>
    <select_all_materials>
        <![CDATA[
            select * from material
        ]]>
    </select_all_materials>

    <select_material_by_sn>
        <![CDATA[
            select * from material where sn = :sn
        ]]>
    </select_material_by_sn>

    <select_material_by_sns>
        <![CDATA[
            select * from material where sn in (:sn)
        ]]>
    </select_material_by_sns>

    <delete_material_by_sn>
        <![CDATA[
            delete from material where sn = :sn
        ]]>
    </delete_material_by_sn>
</SQL>