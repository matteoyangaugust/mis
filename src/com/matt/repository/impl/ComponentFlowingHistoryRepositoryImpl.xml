<?xml version="1.0" encoding="UTF-8"?>
<SQL>
    <select_flowing_histories_by_process_category_sn>
        <![CDATA[
            select * from component_flowing_history where process_category_sn in (:process_category_sn)
        ]]>
    </select_flowing_histories_by_process_category_sn>

    <select_flowing_histories_by_component_sn>
        <![CDATA[
            select * from component_flowing_history where component_sn = :component_sn
        ]]>
    </select_flowing_histories_by_component_sn>

    <delete_flowing_history_by_sn>
        <![CDATA[
            delete from component_flowing_history where sn = :sn
        ]]>
    </delete_flowing_history_by_sn>
</SQL>